{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "prefer-const": false,
        "no-var-requires": false,
        "variable-name": [
            true,
            "allow-leading-underscore",
            "allow-snake-case"
        ],
        "max-classes-per-file": false,
        "no-namespace":false
    },
    "rulesDirectory": []
}